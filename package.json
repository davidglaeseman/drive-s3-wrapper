{
  "name": "drive-s3-wrapper",
  "version": "1.0.3",
  "author": "David Glaeseman",
  "type": "module",
  "description": "Agnostic wrapper for AWS S3 âž” But intended to be used by adonis.js",
  "license": "MIT",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "pubs": "yarn build && yarn test && yarn publish"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/web-components.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/drive-s3-wrapper.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "publishConfig": {
    "@davidglaeseman:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.507.0",
    "@types/mime-types": "^2.1.4",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "dotenv": "^16.4.1",
    "ts-node": "^10.9.2",
    "tsdx": "^0.14.1",
    "typescript": "^5.3.3"
  }
}
